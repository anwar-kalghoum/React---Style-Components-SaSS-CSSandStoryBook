    "@sambego/storybook-styles": "^1.0.0",
    "@svgr/webpack": "2.4.1",
    "@types/storybook-addon-jsx": "^5.4.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.2.3",
    "babel-preset-react-app": "^6.1.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "mini-css-extract-plugin": "0.4.3",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "4.0.1",
    "prop-types": "^15.6.2",
    "puppeteer": "^1.10.0",
    "react": "^16.6.3",
    "react-app-polyfill": "^0.1.3",
    "react-dev-utils": "^6.1.1",
    "react-dom": "^16.6.3",
    "react-modal": "^3.6.1",
    "react-responsive": "^6.0.1",
    "resolve": "1.8.1",
    "sass-loader": "^7.1.0",
    "storybook-addon-jsx": "^5.4.0",
    "styled-components": "^4.1.1",
    "terser-webpack-plugin": "1.1.0",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.9",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "storybook": "start-storybook -p 6006",
    "screenshot": "storybook-chrome-screenshot -p 9001 -c .storybook",
    "build-storybook": "build-storybook",
    "test:generate-output": "jest --json --outputFile=jest-test-results.json",
    "test-storybook": "screener-storybook --conf screener.config.js",
    "chromatic": "chromatic test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@dump247/storybook-state": "^1.5.2",
    "@storybook/addon-a11y": "^4.0.7",
    "@storybook/addon-actions": "^4.0.7",
    "@storybook/addon-backgrounds": "^4.0.7",
    "@storybook/addon-centered": "^4.0.8",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-google-analytics": "^4.0.7",
    "@storybook/addon-info": "^4.0.7",
    "@storybook/addon-jest": "^4.0.8",
    "@storybook/addon-knobs": "^4.0.8",
    "@storybook/addon-links": "^4.0.7",
    "@storybook/addon-notes": "^4.0.7",
    "@storybook/addon-options": "^4.0.7",
    "@storybook/addon-storyshots": "^4.0.8",
    "@storybook/addon-storysource": "^4.0.8",
    "@storybook/addon-viewport": "^4.0.7",
    "@storybook/addons": "^4.0.7",
    "@storybook/react": "^4.0.6",
    "babel-jest": "^23.6.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "css-loader": "^1.0.0",
    "eslint-config-prettier": "^3.3.0",
    "file-loader": "^2.0.0",
    "jest": "^23.6.0",
    "loki": "^0.14.0",
    "node-sass": "^4.10.0",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0",
    "react-test-renderer": "^16.6.3",
    "screener-storybook": "^0.16.2",
    "storybook-addon-figma": "^0.1.0",
    "storybook-addon-i18n-tools": "^1.0.0",
    "storybook-addon-material-ui": "^0.9.0-alpha.16",
    "storybook-addon-smart-knobs": "^3.3.1",
    "storybook-addon-specifications": "^2.1.2",
    "storybook-chromatic": "^1.2.0",
    "storybook-chrome-screenshot": "^1.3.1",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1"
  }
}
